<Group hide-input="false" labelreference="L1" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"># problem 1
# a)
rec := x[n+1] = ((n+1)/(n+2))^2 * x[n] + 1/(n+2):
init := x[0] = 1:
sol := rsolve({rec, init}, x[n]);
# b)
rec := x[n+3] - 4*x[n+2] + x[n+1] + 6*x[n] = 60 * 4^n:
init := x[0] = 2, x[1] = 12, x[2] = 12:
sol := rsolve({rec, init}, x[n]);
# c)
subs_rec := 1/y[n+1] = 2*(1/y[n])/(1 + 4*(1/y[n])):
rec := simplify(subs_rec):
init := y[0] = 1:
sol := rsolve({rec, init}, y[n]):
x_sol := simplify(1/sol);</Text-field>
</Input>
