<Group hide-input="false" labelreference="L2" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"># problem 2
# a)
rec := x[n+1] = (x[n]^2 + 7)/(2*x[n]):
stability := diff((x^2 + 7)/(2*x), x):
eq_points := solve(x = (x^2 + 7)/(2*x), x);
stability_at_eq := [subs(x=eq_points[1], stability),subs(x=eq_points[2], stability)];
# b)
x0 := 1:
num_iters := 25:
results := [x0]:
for i to num_iters do
    results := [op(results), (results[-1]^2 + 7)/(2*results[-1])]:
end do:
plots:-listplot(results, title=&quot;Numerical Simulation&quot;, labels=[&quot;n&quot;, &quot;x[n]&quot;], color=red);</Text-field>
</Input>
