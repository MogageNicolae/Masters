<Group hide-input="false" labelreference="L4" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"># problem 3
# a)
f := x -&gt; x^2 - 3:
e := solve({f(x1) = x2, f(x2) = x1}, {x1, x2});
allvalues(e[3]);
solve(f(x) = x);
# since these values are also equilibrium points, are not 2-period points.
df := diff(f(x), x):
df1 := subs(x=-2, df):
df2 := subs(x=1, df):
prod := simplify(df1 * df2):
stability := abs(prod) &lt; 1;
# b)
x[0] := 1:
N := 50: 
for n from 0 to N-1 do
    x[n+1] := evalf(f(x[n]));
end do:
seq(x[n], n = 0..N):
data := [seq([n, x[n]], n = 0..N)]:
